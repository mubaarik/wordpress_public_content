#: awpcp_category_icons_module.php:40
msgid "Category Icons Module"
msgstr "Módulo de Íconos para Categorías"

#: templates/admin/manage-category-icons-admin-page.tpl.php:56
msgid "Delete Custom Icon"
msgstr "Borar Icono Personalizado"

#: awpcp_category_icons_module.php:46
msgid "Error"
msgstr "Error"

#: awpcp_category_icons_module.php:257
msgid "No icon was selected"
msgstr "No seleccionó ningún ícono"

#: awpcp_category_icons_module.php:43
msgid ""
"The AWPCP <module-name> requires AWPCP version <awpcp-version> or newer!"
msgstr ""
"El modulo <module-name> de AWPCP requiere la versión <awpcp-version> de "
"AWPCP o más nueva!"

#: admin/class-manage-category-icons-admin-page.php:64
msgid ""
"The cat_ID parameter is empty. Please provide the ID of the category you "
"want to modify."
msgstr ""
"El parámetro cat_ID está vacío. Por favor incluya el ID de la catagoría que "
"desea modificar."

#: admin/class-delete-custom-category-icon-ajax-handler.php:54
msgid ""
"The custom icon cannot be deleted because is being used as the category icon "
"for the following categories: <categories>."
msgstr ""
"El ícono personalziado no puede eliminarse porque está siendo usado por las "
"siguientes categorías: <categories>."

#: admin/class-delete-custom-category-icon-ajax-handler.php:75
msgid "The custom icon couldn't be deleted due to an unexpected error."
msgstr ""
"El ícono personalizado no pude ser eliminado debido a un error inesperado."

#: admin/class-manage-category-icons-admin-page.php:92
msgid "The icon for category <category-name> was successfully cleared."
msgstr "El ícono para la categoría <category-name> fue removido exitosamente."

#: admin/class-manage-category-icons-admin-page.php:78
msgid "The icon for category <category-name> was successfully set."
msgstr "El ícono para la categoría <category-name> fue asignado exitosamente."

#: awpcp_category_icons_module.php:252
msgid "The icon for the category <category-name> has been cleared."
msgstr "El ícono para la categoría <category-name> fue removido."

#: awpcp_category_icons_module.php:242
msgid "The icon for the category <category-name> has been set/reset."
msgstr "El ícono para la categoría <category-name> fue asignado."

#: admin/class-upload-custom-category-icon-ajax-handler.php:60
msgctxt "upload custom icon"
msgid ""
"The mime type (<mime-type>) of the custom icon is not allowed or supported."
msgstr ""
"El tipo (<mime-type>) del ícono personalizado no es permitido o no está "
"soportado."

#: admin/class-delete-custom-category-icon-ajax-handler.php:41
msgid "The specified custom icon doesn't exists."
msgstr "El ícono personalizado especificado no existe."

#: admin/class-delete-custom-category-icon-ajax-handler.php:45
msgid "You are not authorized to delete custom category icons."
msgstr "Usted no está autorizado para eliminar íconos personalizados."

#: admin/class-upload-custom-category-icon-ajax-handler.php:39
msgid "You are not authorized to upload custom category icons."
msgstr "Usted no está autorizado para cargar nuevos íconos personalizados."

